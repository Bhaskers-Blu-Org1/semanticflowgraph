@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix monocl: <https://www.datascienceontology.org/ns/monocl/> .


monocl:Type rdf:type rdfs:Class ;
  rdfs:label "Type" ;
  rdfs:comment "A type (alternatively, an object in a category)" .

monocl:Function rdf:type rdfs:Class ;
  rdfs:label "Function" ;
  rdfs:comment "A function (alternatively, a morphism in a category)" .

monocl:inputs rdf:type rdfs:Property ;
  rdfs:label "Inputs of function" .

monocl:outputs rdf:type rdfs:Property ;
  rdfs:label "Outputs of function" .

monocl:subtype_of rdf:type rdfs:Property ;
  rdfs:label "Subtype relation" ;
  rdfs:comment "A subtype (is-a) relation between two types" .

monocl:subfunction_of rdf:type rdfs:Property ;
  rdfs:label "Subfunction relation" ;
  rdfs:comment "A subfunction (is-a) relation between two functions" .


monocl:Concept rdf:type rdfs:Class ;
  rdfs:label "Concept" ;
  rdfs:comment "A concept in a Monocl ontology" .

monocl:TypeConcept rdfs:subClassOf monocl:Concept ;
  rdfs:subClassOf monocl:Type ;
  rdfs:label "Type concept" ;
  rdfs:comment "A type concept in a Monocl ontology" .

monocl:FunctionConcept rdfs:subClassOf monocl:Concept ;
  rdfs:subClassOf monocl:Function ;
  rdfs:label "Function concept" ;
  rdfs:comment "A function concept in a Monocl ontology" .
